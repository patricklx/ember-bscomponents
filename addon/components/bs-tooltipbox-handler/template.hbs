{{import Popover from 'ember-bscomponents/components/-bs-popover'}}
{{import Tooltip from 'ember-bscomponents/components/-bs-tooltip'}}

{{#each popovers as |pop|}}
  <Popover
    @didInsertElementCallback={{fn pop.didInsert pop}}
    @tip_id={{pop.tip_id}}
    @data={{pop.data}}
  />
{{/each}}
{{#each tooltips as |pop|}}
  <Tooltip
    @didInsertElementCallback={{fn pop.didInsert pop}}
    @tip_id={{pop.tip_id}}
    @data={{pop.data}}
  />
{{/each}}
