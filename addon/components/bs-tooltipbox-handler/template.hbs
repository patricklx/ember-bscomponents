{{import Popover from './-bs-popover'}}
{{import Tooltip from './-bs-tooltip'}}

{{#each popovers as |pop|}}
  <Popover
    @didInsertElementCallback={{fn pop.didInsert pop}}
    @tip_id={{pop.tip_id}}
    @data={{pop.data}}
  />
{{/each}}
{{#each tooltips as |pop|}}
  <Tooltip
    @didInsertElementCallback={{fn pop.didInsert pop}}
    @tip_id={{pop.tip_id}}
    @data={{pop.data}}
  />
{{/each}}
