{{import Tabs from './tabs'}}
{{import Pane from './pane'}}

{{yield (hash
  Tabs=(component (ensure-safe-component Tabs)
          manager=this
          selected=this.selected
          content=@content
          onItemSelected=this.onItemSelected
          contentPath=@contentPath
  )
  Pane=(component (ensure-safe-component Pane) manager=this tagName='')
)}}
